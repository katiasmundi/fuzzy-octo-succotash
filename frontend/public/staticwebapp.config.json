{
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ]
}
  